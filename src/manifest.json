{
  "manifest_version": 2,
  "name": "DownloadSelected",
  "version": "0.1",
  "description": "Download selected URLs as Zip",
  "permissions": [
    "contextMenus",
    "activeTab",
    "downloads",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "jszip.min.js",
      "background.js"
    ]
  }
}
