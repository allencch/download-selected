{
  "manifest_version": 3,
  "name": "DownloadSelected",
  "version": "1.0.0",
  "description": "Download selected URLs of images as Zip",
  "permissions": [
    "contextMenus",
    "activeTab",
    "downloads",
    "scripting"
  ],
  "host_permissions": ["*://*/*"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {}
}
